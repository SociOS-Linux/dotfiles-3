"*":
  Zen:
    width: 100
  "atom-ternjs":
    useSnippets: false
    documentation: false
    urls: false
    lint: false
    guess: false
    inlineFnCompletion: false
  "autocomplete-plus":
    confirmCompletion: "tab always, enter when suggestion explicitly selected"
  core:
    excludeVcsIgnoredPaths: false
    disabledPackages: [
      "autocomplete-snippets"
      "autocomplete-atom-api"
    ]
  editor:
    autoIndentOnPaste: false
    invisibles: {}
    softWrap: true
    showIndentGuide: true
  "git-log":
    fontScale: 0.9
  linter:
    errorPanelHeight: 22
  "linter-eslint":
    disableWhenNoEslintrcFileInPath: true
    useGlobalEslint: true
  "markdown-preview": {}
  minimap:
    plugins:
      "git-diff": true
      "find-and-replace": true
      selection: true
      codeglance: true
      bookmarks: true
  "one-dark-ui":
    tabSizing: "Even"
  "spell-check": {}
  tabs:
    usePreviewTabs: true
  "tree-view": {}
  "theme-switcher":
    syntaxThemeOne: "one-dark-syntax"
    syntaxThemeTwo: "one-light-syntax"
  welcome:
    showOnStartup: false
