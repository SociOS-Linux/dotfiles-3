{
  "AllAutocomplete.cycleOpenDocumentsOnLaunch": true,
  "AllAutocomplete.updateOnlyOnSave": true,
  "atomKeymap.promptV3Features": true,
  "breadcrumbs.enabled": false,
  "dayNightThemeSwitcher.dayTheme": "Atom One Light",
  "dayNightThemeSwitcher.nightTheme": "Atomize",
  "diffEditor.renderSideBySide": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.fontFamily": "'SF Mono', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 14,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.hideCursorInOverviewRuler": true,
  "editor.matchBrackets": "near",
  "editor.minimap.enabled": true,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.occurrencesHighlight": false,
  "editor.quickSuggestions": {
    "comments": true,
    "other": true,
    "strings": true
  },
  "editor.quickSuggestionsDelay": 100,
  "editor.roundedSelection": false,
  "editor.scrollBeyondLastLine": false,
  "editor.semanticHighlighting.enabled": false,
  "editor.suggest.maxVisibleSuggestions": 5,
  "editor.suggest.showColors": false,
  "editor.suggest.showIcons": false,
  "editor.suggestOnTriggerCharacters": false,
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "name": "Uncolored",
        "scope": [
          "entity.name",
          "meta.block variable.other",
          "string constant.other.placeholder",
          "string.other.link",
          "support.other.variable",
          "variable.language",
          "variable.other.constant.js",
          "variable.parameter",
          "variable"
        ],
        "settings": {
          "foreground": "#ABB2BF"
        }
      },
      {
        "name": "Red Colored",
        "scope": [
          "meta.object-literal.key",
          "punctuation.definition.template-expression",
          "punctuation.definition.variable.shell",
          "support.variable",
          "variable.other.bracket.shell",
          "variable.other.property"
        ],
        "settings": {
          "foreground": "#E06C75"
        }
      }
    ]
  },
  "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
  "eslint.format.enable": true,
  "explorer.openEditors.visible": 0,
  "extensions.ignoreRecommendations": true,
  "files.autoSave": "onFocusChange",
  "files.trimFinalNewlines": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "grunt.autoDetect": "off",
  "gulp.autoDetect": "off",
  "html.suggest.html5": false,
  "jake.autoDetect": "off",
  "javascript.suggest.completeJSDocs": false,
  "mocha.coverage": {
    "enable": false
  },
  "mocha.options": {
    "ui": "tdd"
  },
  "php.suggest.basic": false,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "typescript.preferences.renameShorthandProperties": false,
  "typescript.reportStyleChecksAsWarnings": false,
  "typescript.suggest.autoImports": false,
  "typescript.suggest.completeJSDocs": false,
  "typescript.suggest.enabled": false,
  "typescript.suggest.includeAutomaticOptionalChainCompletions": false,
  "typescript.suggest.paths": false,
  "typescript.suggestionActions.enabled": false,
  "typescript.surveys.enabled": false,
  "typescript.tsc.autoDetect": "off",
  "typescript.validate.enable": false,
  "update.showReleaseNotes": false,
  "window.newWindowDimensions": "maximized",
  "window.zoomLevel": 0,
  "workbench.activityBar.visible": true,
  "workbench.colorCustomizations": {
    "[Atomize]": {
      "editorBracketMatch.border": "#555",
      "terminal.ansiBlack": "#282C34",
      "terminal.ansiBlue": "#61AFEF",
      "terminal.ansiBrightBlack": "#545862",
      "terminal.ansiBrightBlue": "#61AFEF",
      "terminal.ansiBrightCyan": "#56B6C2",
      "terminal.ansiBrightGreen": "#98C379",
      "terminal.ansiBrightMagenta": "#C678DD",
      "terminal.ansiBrightRed": "#E06C75",
      "terminal.ansiBrightWhite": "#C8CCD4",
      "terminal.ansiBrightYellow": "#E5C07B",
      "terminal.ansiCyan": "#56B6C2",
      "terminal.ansiGreen": "#98C379",
      "terminal.ansiMagenta": "#C678DD",
      "terminal.ansiRed": "#E06C75",
      "terminal.ansiWhite": "#ABB2BF",
      "terminal.ansiYellow": "#E5C07B",
      "terminal.foreground": "#ABB2BF",
      "terminalCursor.background": "#EEE",
      "terminalCursor.foreground": "#ABB2BF77"
    }
  },
  "workbench.colorTheme": "Atomize",
  "workbench.editor.showIcons": false,
  "workbench.editor.showTabs": true,
  "workbench.editor.tabSizing": "shrink",
  "workbench.fontAliasing": "antialiased",
  "workbench.iconTheme": "atom-icons",
  "workbench.preferredDarkColorTheme": "Atomize",
  "workbench.preferredLightColorTheme": "Atom One Light",
  "workbench.startupEditor": "none",
  "workbench.tree.renderIndentGuides": "none"
}
