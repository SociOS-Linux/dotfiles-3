{
  "AllAutocomplete.cycleOpenDocumentsOnLaunch": true,
  "AllAutocomplete.updateOnlyOnSave": true,
  "atomKeymap.promptV3Features": true,
  "breadcrumbs.enabled": false,
  "dayNightThemeSwitcher.dayTheme": "Atom One Light",
  "dayNightThemeSwitcher.nightTheme": "Atomize",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.cursorBlinking": "blink",
  "editor.fontSize": 14,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.hideCursorInOverviewRuler": true,
  "editor.matchBrackets": "near",
  "editor.minimap.enabled": true,
  "editor.minimap.renderCharacters": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.occurrencesHighlight": false,
  "editor.quickSuggestions": {
    "comments": true,
    "other": true,
    "strings": true
  },
  "editor.quickSuggestionsDelay": 100,
  "editor.roundedSelection": false,
  "editor.scrollBeyondLastLine": false,
  "editor.semanticHighlighting.enabled": false,
  "editor.suggest.maxVisibleSuggestions": 5,
  "editor.suggestOnTriggerCharacters": false,
  "editor.tabSize": 2,
  "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
  "eslint.format.enable": true,
  "explorer.openEditors.visible": 0,
  "extensions.ignoreRecommendations": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "git.autofetch": true,
  "git.confirmSync": false,
  "html.suggest.html5": false,
  "javascript.suggest.completeJSDocs": false,
  "php.suggest.basic": false,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "typescript.preferences.renameShorthandProperties": false,
  "typescript.reportStyleChecksAsWarnings": false,
  "typescript.suggest.autoImports": false,
  "typescript.suggest.completeJSDocs": false,
  "typescript.suggest.enabled": false,
  "typescript.suggest.includeAutomaticOptionalChainCompletions": false,
  "typescript.suggest.paths": false,
  "typescript.suggestionActions.enabled": false,
  "typescript.surveys.enabled": false,
  "typescript.tsc.autoDetect": "off",
  "typescript.validate.enable": false,
  "update.showReleaseNotes": false,
  "window.newWindowDimensions": "maximized",
  "window.zoomLevel": 0,
  "workbench.activityBar.visible": true,
  "workbench.colorCustomizations": {
    "editorBracketMatch.border": "#555"
  },
  "workbench.colorTheme": "Atomize",
  "workbench.editor.showIcons": false,
  "workbench.editor.showTabs": true,
  "workbench.editor.tabSizing": "shrink",
  "workbench.fontAliasing": "antialiased",
  "workbench.iconTheme": "atom-icons",
  "workbench.preferredDarkColorTheme": "Atomize",
  "workbench.preferredLightColorTheme": "Atom One Light",
  "workbench.startupEditor": "none",
  "workbench.tree.renderIndentGuides": "none"
}
